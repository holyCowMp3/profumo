{
  "connection": "default",
  "collectionName": "order",
  "info": {
    "name": "order",
    "description": "Заказ | Заказы"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "status": {
      "default": "",
      "type": "enumeration",
      "enum": [
        "payed",
        "error",
        "processing"
      ]
    },
    "owner": {
      "model": "user",
      "via": "orders",
      "plugin": "users-permissions"
    },
    "additionalInfo": {
      "type": "text"
    },
    "orders": {
      "type": "json"
    },
    "deliveryInfo": {
      "type": "json"
    },
    "type": {
      "enum": [
        "nova_poshta",
        "liqpay"
      ],
      "type": "enumeration"
    },
    "comment": {
      "type": "text"
    }
  }
}