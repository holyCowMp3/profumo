{
  "connection": "default",
  "collectionName": "order",
  "info": {
    "name": "order",
    "description": "Заказ | Заказы"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "status": {
      "default": "",
      "type": "enumeration",
      "enum": [
        "Успешно доставлено",
        "Произошла ошибка",
        "В обработке",
        "Передан в службу доставки",
        "Новый заказ",
        "Отказ клиента"
      ]
    },
    "owner": {
      "model": "user",
      "via": "orders",
      "plugin": "users-permissions"
    },
    "additionalInfo": {
      "type": "text"
    },
    "orders": {
      "type": "json"
    },
    "newpost": {
      "type": "json"
    },
    "deliveryInfo": {
      "type": "json"
    },
    "type": {
      "enum": [
        "nova_poshta",
        "liqpay"
      ],
      "type": "enumeration"
    }
  }
}
