{
  "connection": "default",
  "collectionName": "order",
  "info": {
    "name": "order",
    "description": "Заказ | Заказы"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "dateOrder": {
      "default": "",
      "type": "date"
    },
    "products": {
      "collection": "product",
      "via": "order"
    },
    "status": {
      "default": "",
      "type": "enumeration",
      "enum": [
        "payed",
        "error",
        "processing"
      ]
    },
    "owner": {
      "model": "user",
      "via": "orders",
      "plugin": "users-permissions"
    },
    "additionalInfo": {
      "type": "text"
    }
  }
}